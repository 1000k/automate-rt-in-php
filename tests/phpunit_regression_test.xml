<?xml version="1.0" encoding="UTF-8"?>

<phpunit colors="false"
    backupGlobals="false"
    backupStaticAttributes="false"
    strict="true"
    verbose="true">
  <testsuites>
    <testsuite name="EMS Regression Test">
      <directory suffix="Test.php">./selenium/</directory>
    </testsuite>
  </testsuites>

  <logging>
    <log type="junit" target="build/logs/junit-selenium.xml" logIncompleteSkipped="false"/>
  </logging>

</phpunit>
